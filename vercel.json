{
    "version": 2,
    "buildCommand": "npm run build",
    "installCommand": "npm install",
    "framework": null,
    "outputDirectory": "dist/public",
    "routes": [
        {
            "src": "/api/contact",
            "methods": [
                "POST"
            ],
            "dest": "/api/index.ts"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}