{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "dist/public",
    "builds": [
        {
            "src": "api/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/index.ts"
        },
        {
            "src": "/(.*)",
            "dest": "api/index.ts"
        }
    ]
}